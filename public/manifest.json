{
  "manifest_version": 3,
  "name": "Cookies List",
  "version": "1.0",
  "description": "View all your cookies",
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "cookies",
    "proxy",
    "storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
